    "print(\"find probility of card\\n\\n\")\n",
    "cards=52\n",
    "spade=13\n",
    "king=4\n",
    "neither=cards-(spade+king-1)\n",
    "\n",
    "probility=(neither/cards)\n",
    "\n",
    "print(\"your probility is :\", probility)\n"
   ]

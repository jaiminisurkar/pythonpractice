   "source": [
    "number=int(input(\"Enter any number: \"))\n",
    "count=0\n",
    "for i in range(1,(number // 2) + 1):\n",
    "    if number%i==0:\n",
    "        count=count+i\n",
    "        print(i,end=\" \")\n",
    "\n",
    "if count==number:\n",
    "    print(\"\\nPerfact:\",number)\n",
    "else:\n",
    "    print(\"\\nNot Perfact:\",number)\n"
   ]
